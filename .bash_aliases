#These aliases are made specifically for my computer.
#Which is Running Ubuntu 13.10 64bit with Unity. 
#Some of these may not work on your computer.

#Apt Aliases
alias apt-uu='sudo apt-get update; sudo apt-get upgrade -y'
alias install='sudo apt-get install'

#Bundler Aliases
alias bune='bundle exec'
alias bune-db='bundle exec rake db:migrate'

#Git Aliases
alias ga='git add'
alias gc='git commit -m'
alias gi='git init'
alias gpush='git push'
alias gpushfirst="git push -u origin master"
alias gra='git remote add origin'
alias gs='git status'

#Nautilus Aliases
alias nopen='nautilus'

#Rails Aliases
alias rgm='rails generate migration'
